{
    "plugins": ["import"],
    "env": {
        "jest": true,
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "extends": [
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "rules": {
        "import/namespace": "off",
        "require-await": "off"
    }
}