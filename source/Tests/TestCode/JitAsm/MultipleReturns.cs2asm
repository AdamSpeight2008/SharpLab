static class C {
    static int M(bool x) {
        return x ? 1 : 2;
    }
}

#=>

; This is an experimental implementation.
; Please report any bugs to https://github.com/ashmind/TryRoslyn/issues.

C.M(Boolean)
    L0000: test cl, cl
    L0002: jnz L000a
    L0004: mov eax, 0x2
    L0009: ret
    L000a: mov eax, 0x1
    L000f: ret